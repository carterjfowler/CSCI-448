<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/locatrFragment">

    <fragment
        android:id="@+id/locatrFragment"
        android:name="com.carterjfowler.geolocatr.LocatrFragment"
        android:label="LocatrFragment"
        tools:layout="@layout/fragment_locatr">
        <argument
            android:name="longitude"
            app:argType="float"
            android:defaultValue="-999.0" />
        <argument
            android:name="latitude"
            app:argType="float"
            android:defaultValue="-999.0" />
    </fragment>
</navigation>